# shellcheck shell=bash

# ``````````````````````````````````````````````````````````````````````````````
# Function name: getGitRepo()
#
# Description:
#   Download github user repository.
#
# Usage:
#   getGitRepo
#
# Examples:
#   getGitRepo
#

function getGitRepo() {

  local _FUNCTION_ID="getGitRepo"
  local _STATE="0"

  for i in $(cat ${_output_tmp}) ; do

    echo $i

  done

  return "$_STATE"

}
